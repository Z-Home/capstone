<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/scenariosFragment"
    >
    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scenariosListView">
    </ListView>

    <ImageView
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:baselineAlignBottom="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="10dp"
        android:src="@drawable/gold_circle"

        />
    <ImageButton
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:baselineAlignBottom="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="10dp"
        android:src="@drawable/add"
        android:background="@android:color/transparent"
        android:id="@+id/addScenarioBtn"
        />

    <Button
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:baselineAlignBottom="true"
        android:layout_alignParentBottom="true"
        android:layout_margin="10dp"
        android:src="@drawable/add"
        android:background="@android:color/transparent"
        android:id="@+id/testBtn"
        android:text="Scenario Setup View"
        />
</RelativeLayout><!-- From: file:/Users/broccard/Documents/School/Fall%202014/Capstone%20II/capstone/capstone/Android/ZHOME/app/src/main/res/layout/fragment_scenarios.xml -->